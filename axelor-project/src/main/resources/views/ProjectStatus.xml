<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

	<grid name="project-status-grid" title="Status" model="com.axelor.apps.project.db.ProjectStatus" editable="true" orderBy="sequence">
	    <field name="sequence"/>
		<field name="name"/>
		<field name="isDefaultCompleted"/>
		<field name="isCompleted"/>
	</grid>
	
	<form name="project-status-form" title="Status" model="com.axelor.apps.project.db.ProjectStatus">
		<panel name="mainPanel">
		    <field name="sequence"/>
			<field name="name"/>
			<field name="isDefaultCompleted"/>
			<field name="isCompleted"/>
		</panel>
	</form>
	
</object-views>